{
    "compilerOptions": {
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "strict": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "baseUrl": "./",
        "paths": {
            "@": [
                "src"
            ],
            "vue-property-decorator": [
                "/node_modules/vue-property-decorator/lib/vue-property-decorator"
            ]
        }
    }
}
